{
    "source": {
        "include": ["./src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf-8",
        "readme": "./README.md",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true,
        "template": "./minami"
    },
    "base": "/jsoning/"
}